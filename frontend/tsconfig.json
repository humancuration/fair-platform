{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@store/*": ["store/*"],
      "@pages/*": ["pages/*"],
      "@providers/*": ["providers/*"],
      "@models/*": ["models/*"]
    },
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react-jsx"
    // ... other configurations
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}